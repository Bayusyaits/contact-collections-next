{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "ignorePatterns": ["**/static/*.json","*.json"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": ["react"]
}
